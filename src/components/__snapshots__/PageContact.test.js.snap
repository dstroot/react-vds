exports[`PageContact renders the landing page 1`] = `
<div
  className="container"
>
  <div
    className="row"
  >
    <div
      className="col-md-8 offset-md-2 text-center mt-5 bd-text-purple-bright"
    >
      <h2
        className="font-weight-light"
      >
        Thanks for Reaching Out!
      </h2>
      <p
        className="lead"
      >
        We would love to meet and learn more about your team, your unique environment, and your planned journey.
      </p>
    </div>
  </div>
  <form
    className=""
    noValidate={true}
    onSubmit={[Function]}
  >
    <div
      className="form-row"
    >
      <div
        className="col-md-6 offset-md-3"
      >
        <input
          className="invisible"
          name="_gotcha"
          type="text"
        />
        <input
          name="_subject"
          type="hidden"
          value="Website Contact"
        />
        <h3
          className="mt-3 font-weight-light"
        >
          Contact Us:
        </h3>
        <div
          className="form-group"
        >
          <label
            htmlFor="email"
          >
            Name
          </label>
          <input
            className="form-control"
            data-parse="trim"
            name="name"
            pattern=".*\\\\S+.*"
            placeholder="Please enter your name"
            required={true}
            type="text"
          />
          <div
            className="invalid-feedback"
          >
            Please enter your name.
          </div>
          <div
            className="valid-feedback"
          >
            Looks good!
          </div>
        </div>
        <div
          className="form-group"
        >
          <label
            htmlFor="email"
          >
            Email address
          </label>
          <input
            className="form-control"
            name="email"
            placeholder="you@youremail.com"
            required={true}
            type="email"
          />
          <div
            className="invalid-feedback"
          >
            Please enter a valid email address.
          </div>
          <div
            className="valid-feedback"
          >
            Looks good!
          </div>
        </div>
        <div
          className="form-group"
        >
          <label
            htmlFor="email"
          >
            Phone Number
          </label>
          <input
            className="form-control"
            name="phone"
            pattern="^[0-9-+s()]*$"
            placeholder="(123) 456-7890"
            type="tel"
          />
          <div
            className="invalid-feedback"
          >
            Please enter a valid phone number.
          </div>
          <div
            className="valid-feedback"
          >
            optional
          </div>
        </div>
        <div
          className="form-group"
        >
          <label
            htmlFor="email"
          >
            Short Message
          </label>
          <textarea
            className="form-control"
            data-parse="trim"
            name="message"
            onChange={[Function]}
            pattern=".*\\\\S+.*"
            placeholder="Please enter a short message"
            required={true}
          />
          <div
            className="invalid-feedback"
          >
            Please let us know how we can help!
          </div>
          <div
            className="valid-feedback"
          >
            Looks good!
          </div>
        </div>
        <div
          className="form-row"
        >
          <div
            className="col-md-5"
          >
            <button
              className="btn btn-primary btn-block"
              type="submit"
            >
              <i
                className="fa"
              />
               Â  Submit
            </button>
          </div>
        </div>
      </div>
    </div>
  </form>
  
</div>
`;
